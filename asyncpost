const { request, response, Router } = require('express');
const contact = require('./asyncpost')
const express = require('express');
const { db } = require('./config');
var router = express.Router();
const config = require('./config');
const conn = require('./db');
const { query } = require('./db');
const app = express();
async function create(contact){
    const result = await conn(
    'Insert into contact.contacts(PersonID, LastName, FirstName, Address, City) VALUES (?,?,?,?,?)',
  [
    contact.PersonID, contact.LastName, contact.FirstName, contact.Address, contact.City
  ]
 
  );
  let message = 'Error in creating contact';
  if (result.affectedRows) {
    message = 'Contact created succesfully!';
  }
    return {message};
}
module.exports = {
    create
  }